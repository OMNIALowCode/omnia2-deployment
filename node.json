{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "samymisname": {
      "type": "string"
    },
    "wsmymisnodeName": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "appsrvplanmymisName": {
      "type": "string"
    },
    "wsmymisName": {
      "type": "string"
    },
    "samymiskey": {
      "type": "string"
    }
  },
  "variables": {
  },
  "resources": [
    {
      "name": "[parameters('wsmymisnodeName')]",
      "type": "Microsoft.Web/sites",
      "location": "[parameters('location')]",
      "apiVersion": "2015-08-01",
      "tags": {
        "[concat('hidden-related:', resourceGroup().id, '/providers/Microsoft.Web/serverfarms/', parameters('appsrvplanmymisName'))]": "Resource",
        "displayName": "wsmymisnode"
      },
      "properties": {
        "name": "[parameters('wsmymisnodeName')]",
        "serverFarmId": "[resourceId('Microsoft.Web/serverfarms/', parameters('appsrvplanmymisName'))]"
      },
      "resources": [
        {
          "apiVersion": "2015-08-01",
          "name": "MSDeploy",
          "type": "extensions",
          "location": "[resourceGroup().location]",
          "dependsOn": [
            "[concat('Microsoft.Web/Sites/', parameters('wsmymisnodeName'))]"
          ],
          "properties": {
            "packageUri": "https://mymiswebdeploy.blob.core.windows.net/artifacts/MyMis.Extensibility.Engine.zip",
            "setParameters": {
              "IIS Web Application Name": "[parameters('wsmymisnodeName')]"
            }
          }
        },
        {
          "name": "appsettings",
          "apiVersion": "2015-08-01",
          "type": "config",
          "dependsOn": [
            "[concat('Microsoft.Web/Sites/', parameters('wsmymisnodeName'))]",
            "[concat('Microsoft.Web/Sites/', parameters('wsmymisnodeName'), '/extensions/MSDeploy')]"
          ],
          "properties": {
            "api_endpoint": "[parameters('wsmymisName')]",
            "api_oauth_id": "7b6a25222e65234e2d3c32616e",
            "api_oauth_secret": "593d29374f694d6424524d7e7e6e657327433e2b4b2b5a3b473c325434",
            "logging_azure_development_storage": "0",
            "logging_azure_storage_account": "[parameters('samymisName')]",
            "logging_azure_storage_account_key": "[parameters('samymiskey')]",
            "scripts_azure_development_storage": "0",
            "scripts_azure_storage_account": "[parameters('samymisName')]",
            "scripts_azure_storage_account_key": "[parameters('samymiskey')]",
            "communication_server_endpoint": "***REMOVED***",
            "communication_server_version": "1.0.0",
            "communication_server_secret": "***REMOVED***",
            "logging_logging_level": "debug",
            "WEBSITE_NODE_DEFAULT_VERSION": "4.4.7"
          }
        },
        {
          "name": "web",
          "apiVersion": "2015-08-01",
          "type": "config",
          "dependsOn": [
            "[concat('Microsoft.Web/Sites/', parameters('wsmymisnodeName'))]",
            "[concat('Microsoft.Web/Sites/', parameters('wsmymisnodeName'), '/extensions/MSDeploy')]",
            "[concat('Microsoft.Web/Sites/', parameters('wsmymisnodeName'), '/config/appsettings')]"
          ],
          "properties": {
            "webSocketsEnabled": true
          }
        }
      ]
    }
  ],
  "outputs": {
  }
}
