{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "certificate": {
      "type": "string",
    },
    "certificateThumbprint": {
      "type": "string",
      "defaultValue": "E17940DAB14E322B0A5EC6EAEDF893A64CB9AFBE"
    },
    "lpContactEmail": {
      "type": "string"
    },
    "sqlsrvmymisAdminLoginPassword": {
      "type": "securestring"
    },
    "sqldbmymisCollation": {
      "type": "string",
      "minLength": 1,
      "defaultValue": "SQL_Latin1_General_CP1_CI_AS"
    },
    "sqldbmymisEdition": {
      "type": "string",
      "defaultValue": "Standard",
      "allowedValues": [
        "Basic",
        "Standard",
        "Premium"
      ]
    },
    "sqldbmymisRequestedServiceObjectiveName": {
      "type": "string",
      "defaultValue": "S0",
      "allowedValues": [
        "Basic",
        "S0",
        "S1",
        "S2",
        "P1",
        "P2",
        "P3"
      ],
      "metadata": {
        "description": "Describes the performance level for Edition"
      }
    },
    "appsrvplanmymisWorkerSize": {
      "type": "string",
      "allowedValues": [
        "0",
        "1",
        "2"
      ],
      "defaultValue": "0"
    },
    "appsrvplanmymisSKU": {
      "type": "string",
      "allowedValues": [
        "Free",
        "Shared",
        "Basic",
        "Standard"
      ],
      "defaultValue": "Basic"
    },
    "mymisStorageType": {
      "type": "string",
      "defaultValue": "Standard_LRS",
      "allowedValues": [
        "Standard_LRS",
        "Standard_ZRS",
        "Standard_GRS",
        "Standard_RAGRS",
        "Premium_LRS"
      ]
    },
    "sendGridUrl": {
      "type": "string"
    },
    "sendGridUsername": {
      "type": "string"
    },
    "sendGridPassword": {
      "type": "string"
    },
    "masterEmail": {
      "type": "string"
    },
    "branding": {
      "type": "string"
    },
    "defaultOem": {
      "type": "string"
    },
    "gAnalytics": {
      "type": "string"
    },
    "oAuthId": {
      "type": "string"
    },
    "oAuthSecret": {
      "type": "string"
    },
    "ms_Id": {
      "type": "string"
    },
    "ms_Secret": {
      "type": "string"
    },
    "twitter_Key": {
      "type": "string"
    },
    "twitter_Secret": {
      "type": "string"
    },
    "fb_Id": {
      "type": "string"
    },
    "fb_Secret": {
      "type": "string"
    },
    "linkedin_Key": {
      "type": "string"
    },
    "linkedin_Secret": {
      "type": "string"
    },
    "SQL_Management_StartIP": {
      "type": "string"
    },
    "SQL_Management_EndIP": {
      "type": "string"
    },

    "SQL_Management_StartIP2": {
      "type": "string"
    },
    "SQL_Management_EndIP2": {
      "type": "string"
    },
    "administrationUsers": {
      "type": "string"
    },
    "administrationUserPasswords": {
      "type": "string"
    },
    "fileSize": {
      "type": "string",
      "defaultValue": "10"
    }
  },
  "variables": {
    "location": "[resourceGroup().location]",
    "sqlsrvmymisName": "[concat('sqlsrvmymis', tolower(resourceGroup().name))]",
    "sqlsrvmymisAdminLogin": "[concat('sqlsrvmymisAdminLogin', tolower(resourceGroup().name))]",
    "sqldbmymisName": "[concat('sqldbmymis', tolower(resourceGroup().name))]",
    "wsmymisName": "[concat('wsmymis', tolower(resourceGroup().name))]",
    "wsmymisnodeName": "[concat('wsmymisnode', tolower(resourceGroup().name))]",
    "appsrvplanmymisName": "[concat('appsrvplanmymis', tolower(resourceGroup().name))]",
    "samymisName": "[concat('samymis', tolower(resourceGroup().name))]"
  },
  "resources": [
    {
      "name": "[variables('sqlsrvmymisName')]",
      "type": "Microsoft.Sql/servers",
      "location": "[resourceGroup().location]",
      "apiVersion": "2014-04-01-preview",
      "dependsOn": [ ],
      "tags": {
        "displayName": "sqlsrvmymis"
      },
      "properties": {
        "version": "12.0",
        "administratorLogin": "[variables('sqlsrvmymisAdminLogin')]",
        "administratorLoginPassword": "[parameters('sqlsrvmymisAdminLoginPassword')]"
      },
      "resources": [
        {
          "name": "AllowAllWindowsAzureIps",
          "type": "firewallrules",
          "location": "[resourceGroup().location]",
          "apiVersion": "2014-04-01-preview",
          "dependsOn": [
            "[concat('Microsoft.Sql/servers/', variables('sqlsrvmymisName'))]"
          ],
          "properties": {
            "startIpAddress": "0.0.0.0",
            "endIpAddress": "0.0.0.0"
          }
        },
        {
          "name": "AllowIP1",
          "type": "firewallrules",
          "location": "[resourceGroup().location]",
          "apiVersion": "2014-04-01-preview",
          "dependsOn": [
            "[concat('Microsoft.Sql/servers/', variables('sqlsrvmymisName'))]"
          ],
          "properties": {
            "startIpAddress": "[parameters('SQL_Management_StartIP')]",
            "endIpAddress": "[parameters('SQL_Management_EndIP')]"
          }
        },
        {
          "name": "AllowIP2",
          "type": "firewallrules",
          "location": "[resourceGroup().location]",
          "apiVersion": "2014-04-01-preview",
          "dependsOn": [
            "[concat('Microsoft.Sql/servers/', variables('sqlsrvmymisName'))]"
          ],
          "properties": {
            "startIpAddress": "[parameters('SQL_Management_StartIP2')]",
            "endIpAddress": "[parameters('SQL_Management_EndIP2')]"
          }
        },
        {
          "name": "[variables('sqldbmymisName')]",
          "type": "databases",
          "location": "[resourceGroup().location]",
          "apiVersion": "2014-04-01-preview",
          "dependsOn": [
            "[variables('sqlsrvmymisName')]"
          ],
          "tags": {
            "displayName": "sqldbmymis"
          },
          "properties": {
            "collation": "[parameters('sqldbmymisCollation')]",
            "edition": "[parameters('sqldbmymisEdition')]",
            "maxSizeBytes": "1073741824",
            "requestedServiceObjectiveName": "[parameters('sqldbmymisRequestedServiceObjectiveName')]"
          }
        }
      ]
    },
    {
      "name": "[variables('appsrvplanmymisName')]",
      "type": "Microsoft.Web/serverfarms",
      "location": "[resourceGroup().location]",
      "apiVersion": "2014-06-01",
      "dependsOn": [ ],
      "tags": {
        "displayName": "appsrvplanmymis"
      },
      "properties": {
        "name": "[variables('appsrvplanmymisName')]",
        "sku": "[parameters('appsrvplanmymisSKU')]",
        "workerSize": "[parameters('appsrvplanmymisWorkerSize')]",
        "numberOfWorkers": 1
      }
    },
    {
      "name": "[variables('wsmymisName')]",
      "type": "Microsoft.Web/sites",
      "location": "[resourceGroup().location]",
      "apiVersion": "2015-08-01",
      "dependsOn": [
        "[concat('Microsoft.Web/serverfarms/', variables('appsrvplanmymisName'))]"
      ],
      "tags": {
        "[concat('hidden-related:', resourceGroup().id, '/providers/Microsoft.Web/serverfarms/', variables('appsrvplanmymisName'))]": "Resource",
        "displayName": "wsmymis"
      },
      "properties": {
        "name": "[variables('wsmymisName')]",
        "serverFarmId": "[resourceId('Microsoft.Web/serverfarms/', variables('appsrvplanmymisName'))]"
      }
    },
    {
      "name": "[variables('samymisName')]",
      "type": "Microsoft.Storage/storageAccounts",
      "location": "[resourceGroup().location]",
      "apiVersion": "2015-06-15",
      "dependsOn": [ ],
      "tags": {
        "displayName": "samymis"
      },
      "properties": {
        "accountType": "[parameters('mymisStorageType')]"
      }
    },
    {
      "name": "[variables('wsmymisnodeName')]",
      "type": "Microsoft.Web/sites",
      "location": "[resourceGroup().location]",
      "apiVersion": "2015-08-01",
      "dependsOn": [
        "[concat('Microsoft.Web/serverfarms/', variables('appsrvplanmymisName'))]"
      ],
      "tags": {
        "[concat('hidden-related:', resourceGroup().id, '/providers/Microsoft.Web/serverfarms/', variables('appsrvplanmymisName'))]": "Resource",
        "displayName": "wsmymisnode"
      },
      "properties": {
        "name": "[variables('wsmymisnodeName')]",
        "serverFarmId": "[resourceId('Microsoft.Web/serverfarms/', variables('appsrvplanmymisName'))]"
      }
    }
  ],
  "outputs": {
    "certificate": {
      "type": "string",
      "value": "[parameters('certificate')]"
    },
    "certificateThumbprint": {
      "type": "string",
      "value": "[parameters('certificateThumbprint')]"
    },
    "lpContactEmail": {
      "type": "string",
      "value": "[parameters('lpContactEmail')]"
    },
    "saConnectionString": {
      "type": "string",
      "value": "[concat('DefaultEndpointsProtocol=https;AccountName=', variables('samymisName'), ';AccountKey=', listKeys(resourceId('Microsoft.Storage/storageAccounts', variables('samymisName')), '2015-05-01-preview').key1)]"
    },
    "saName": {
      "type": "string",
      "value": "[variables('samymisName')]"
    },
    "saKey": {
      "type": "string",
      "value": "[listKeys(resourceId('Microsoft.Storage/storageAccounts', variables('samymisName')), '2015-05-01-preview').key1]"
    },
    "sqlServer": {
      "type": "string",
      "value": "[variables('sqlsrvmymisName')]"
    },
    "sqlDatabase": {
      "type": "string",
      "value": "[variables('sqldbmymisName')]"
    },
    "sqlAdmin": {
      "type": "string",
      "value": "[variables('sqlsrvmymisAdminLogin')]"
    },
    "sqlAdminPassword": {
      "type": "string",
      "value": "[parameters('sqlsrvmymisAdminLoginPassword')]"
    },
    "webAppUrl": {
      "type": "string",
      "value": "[concat(variables('wsmymisName'), '.azurewebsites.net')]"
    },
    "webExtensibility": {
      "type": "string",
      "value": "[concat(variables('wsmymisnodeName'), '.azurewebsites.net')]"
    },
    "sendGridUrl": {
      "type": "string",
      "value": "[parameters('sendGridUrl')]"
    },
    "sendGridUsername": {
      "type": "string",
      "value": "[parameters('sendGridUsername')]"
    },
    "sendGridPassword": {
      "type": "string",
      "value": "[parameters('sendGridPassword')]"
    },
    "masterEmail": {
      "type": "string",
      "value": "[parameters('masterEmail')]"
    },
    "branding": {
      "type": "string",
      "value": "[parameters('branding')]"
    },
    "defaultOem": {
      "type": "string",
      "value": "[parameters('defaultOem')]"
    },
    "gAnalytics": {
      "type": "string",
      "value": "[parameters('gAnalytics')]"
    },
    "oAuthId": {
      "type": "string",
      "value": "[parameters('oAuthId')]"
    },
    "oAuthSecret": {
      "type": "string",
      "value": "[parameters('oAuthSecret')]"
    },
    "ms_Id": {
      "type": "string",
      "value": "[parameters('ms_Id')]"
    },
    "ms_Secret": {
      "type": "string",
      "value": "[parameters('ms_Secret')]"
    },
    "twitter_Key": {
      "type": "string",
      "value": "[parameters('twitter_Key')]"
    },
    "twitter_Secret": {
      "type": "string",
      "value": "[parameters('twitter_Secret')]"
    },
    "fb_Id": {
      "type": "string",
      "value": "[parameters('fb_Id')]"
    },
    "fb_Secret": {
      "type": "string",
      "value": "[parameters('fb_Secret')]"
    },
    "linkedin_Key": {
      "type": "string",
      "value": "[parameters('linkedin_Key')]"
    },
    "linkedin_Secret": {
      "type": "string",
      "value": "[parameters('linkedin_Secret')]"
    },
    "administrationUsers": {
      "type": "string",
      "value": "[parameters('administrationUsers')]"
    },
    "administrationUserPasswords": {
      "type": "string",
      "value": "[parameters('administrationUserPasswords')]"
    },
    "fileSize": {
      "type": "string",
      "value": "[parameters('fileSize')]"
    }
  }
}
